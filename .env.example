# Google Search API
GOOGLE_API_KEY=your_api_key_here
GOOGLE_CX=your_cx_here
GOOGLE_REQUESTS_PER_MINUTE=100
GOOGLE_REQUESTS_PER_DAY=10000
GOOGLE_MAX_RESULTS_PER_QUERY=10
GOOGLE_MAX_CONCURRENT=50  # Max parallel API requests (high is fine, rate limiter controls throughput)
GOOGLE_FREE_TIER_ONLY=false  # Set to true to limit to 100 requests/day (free tier)

# Crawl4AI - Adaptive Concurrency
CRAWLER_MAX_CONCURRENCY=10  # Upper limit - dispatcher scales down based on memory
CRAWLER_MEMORY_THRESHOLD_PERCENT=85  # Reduce concurrency when memory exceeds this %

# Crawl4AI - Retry/Backoff
CRAWLER_RETRY_MAX=3
CRAWLER_RETRY_BASE_DELAY_MIN=1.0
CRAWLER_RETRY_BASE_DELAY_MAX=2.0
CRAWLER_RETRY_MAX_DELAY=30.0

# Crawl4AI - Content Processing
CRAWLER_USE_PRUNING_FILTER=true
CRAWLER_PRUNING_THRESHOLD=0.3
CRAWLER_HEADLESS=true

# Pipeline
PIPELINE_QUEUE_SIZE=100
PIPELINE_BATCH_SIZE=10
